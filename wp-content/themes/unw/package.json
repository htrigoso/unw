{
  "author": "Mike Opatskyi <michael@seek.pe> (https://www.seek.pe)",
  "name": "unw",
  "version": "0.1.0",
  "description": "",
  "homepage": "https://bitbucket.org/seekww/unw-website/README.md",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:seekww/unw-website.git"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "resolutions": {
    "css-loader": "^6.7.1"
  },
  "browserslist": [
    "last 4 versions",
    "Firefox ESR",
    "android 4",
    "opera 12",
    "ie >= 10",
    "ie_mob >= 10",
    "ff >= 30",
    "chrome >= 34",
    "safari >= 7",
    "opera >= 23",
    "ios >= 7",
    "bb >= 10",
    "not dead"
  ],
  "target": "web",
  "scripts": {
    "dev": "npm run cleanup && webpack-dev-server --progress --mode=development --allowed-hosts all",
    "build": "npm run cleanup && webpack --progress --mode=production",
    "watch": "npm run cleanup && webpack --mode=development --watch",
    "check-format": "prettier --check ./src/\"**/*.{js,css,scss,json}\"",
    "check-lint": "eslint ./src/\"**/*.{js,json}\"",
    "format": "prettier --write ./src/\"**/*.{js,json}\"",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types",
    "cleanup": "rm -r -f build && clear",
    "postinstall": "cd ../../../ && husky install",
    "php:check": "vendor/bin/phpcs --standard=phpcs.xml --extensions=php .",
    "php:fix": "vendor/bin/phpcbf --standard=phpcs.xml --extensions=php ."
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@wordpress/dependency-extraction-webpack-plugin": "^4.9.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-webpack-plugin": "^4.0.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^10.0.1",
    "imports-loader": "^4.0.1",
    "jsconfig.json": "^2.3.3",
    "json-loader": "^0.5.7",
    "lint-staged": "^13.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "npm": "^9.4.1",
    "plyr": "^3.7.8",
    "postcss": "^8.4.31",
    "postcss-custom-properties": "^13.1.1",
    "postcss-loader": "^7.3.3",
    "postcss-scss": "^4.0.6",
    "prefix": "^1.0.0",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.89.2",
    "sass-lint-webpack": "^1.0.4",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-webpack-plugin": "^4.0.0",
    "terser-webpack-plugin": "^5.3.6",
    "url-loader": "^4.1.1",
    "webpack": "^5.82.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^5.0.0",
    "webpack-nano": "^1.1.1"
  },
