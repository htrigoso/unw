    "auto-bind": "^5.0.1",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "events": "^3.3.0",
    "include-media": "^2.0.0",
    "intersection-observer": "^0.12.2",
    "jquery": "^3.6.4",
    "jquery-validation": "^1.19.5",
    "lit-popup": "^1.8.0",
    "lodash": "^4.17.21",
    "rxjs": "^7.8.0",
    "scrollmagic": "^2.0.8",
    "simplebar": "^6.2.0",
    "slim-js": "^5.0.10",
    "smoothscroll-polyfill": "^0.4.4",
    "swiper": "^8.3.1",
    "yup": "^0.32.11"
  }
}
